set -e

PACKAGE="gites-website-core"


case "$1" in
    remove|deconfigure)
        find /home/gdw/gites-website/eggs -name "*.pyc" -delete;
	find /home/gdw/gites-website/eggs -name "*.pyo" -delete;
    ;;

    upgrade|failed-upgrade)
    ;;

    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 0
    ;;
esac

exit 0
