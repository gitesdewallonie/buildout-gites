#!/bin/sh
set -e
set -x

PACKAGE="gites-bnb-core"


case "$1" in
    remove|deconfigure)
	find /home/gdw/gites-bnb/eggs /home/gdw/gites-bnb/devel -name "*.pyc" -delete;
	find /home/gdw/gites-bnb/eggs /home/gdw/gites-bnb/devel -name "*.pyo" -delete;
	find /home/gdw/gites-bnb/eggs /home/gdw/gites-bnb/devel -name "*.bin" -delete;
    ;;

    upgrade|failed-upgrade)
    ;;

    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 0
    ;;
esac

exit 0
